{
  "name": "nummus-app",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "build": "turbo run build",
    "dev": "turbo run dev --parallel --force",
    "test": "turbo run test"
  },
  "devDependencies": {
    "eslint": "^8.33.0",
    "turbo": "1.9.3",
    "typescript": "5.1.6"
  },
  "engines": {
    "node": ">=16"
  },
  "packageManager": "pnpm@8.15.1"
}
